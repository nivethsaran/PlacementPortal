<xs:schema attributeFormDefault="unqualified" elementFormDefault="qualified" xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:element name="scores">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="score" maxOccurs="unbounded" minOccurs="0">
          <xs:complexType>
            <xs:sequence>
              <xs:element type="xs:int" name="scoreid"/>
              <xs:element type="xs:int" name="quizid"/>
              <xs:element type="xs:string" name="rollno"/>
              <xs:element type="xs:int" name="total"/>
              <xs:element type="xs:int" name="studentscore" />
            </xs:sequence>
          </xs:complexType>
        </xs:element>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
</xs:schema>